<Window x:Class="WPF_HikvisionCamera.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_HikvisionCamera"
        mc:Ignorable="d"
        Title="WPF_海康照相机" Height="300" Width="300">
    <StackPanel Margin="0">
        <Button x:Name="button_Image"  Padding="5" Content="WPF Image控件实现" Margin="0,10,0,0" Click="button_Image_Click"/>
        <Button x:Name="button_WinFormPicBox"  Padding="5" Content="WindowsForm的PictureBox实现" Margin="0,10,0,0" Click="button_WinFormPicBox_Click"/>
    </StackPanel>

</Window>
